  "info": {
    "title": "API Template",
    "description": "Reusable NestJS API template for microservices",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "UpdateUserDto": {
        "type": "object",
        "properties": {
          "displayName": {
            "type": "string",
            "description": "Updated display name for the user",
            "example": "Jane Doe"
          },
          "avatarUrl": {
            "type": "string",
            "description": "URL of the new avatar image",
            "example": "https://cdn.example.com/avatars/janedoe.jpg",
            "format": "uri"
          }
        }
      },
      "RegisterDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "User email address",
            "example": "newuser@example.com",
            "format": "email"
          },
          "password": {
            "type": "string",
            "description": "Password (minimum 6 characters)",
            "example": "supersecret",
            "minLength": 6
          },
          "displayName": {
            "type": "string",
            "description": "Display name of the user",
            "example": "John Doe"
          }
        },
        "required": [
          "email",
          "password",
          "displayName"
        ]
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "User email address",
            "example": "user@example.com",
            "format": "email"
          },
          "password": {
            "type": "string",
            "description": "User password (minimum 6 characters)",
            "example": "strongpassword",
            "minLength": 6
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "CreateProductDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Name of the product",
            "example": "Wireless Headphones"
          },
          "description": {
            "type": "string",
            "description": "Detailed description of the product",
            "example": "Noise-cancelling over-ear Bluetooth headphones."
          },
          "price": {
            "type": "number",
            "description": "Price of the product (must be non-negative)",
            "example": 129.99,
            "minimum": 0
          },
          "stock": {
            "type": "number",
            "description": "Stock quantity available",
            "example": 50,
            "minimum": 0
          },
          "imageUrl": {
            "type": "string",
            "description": "Image URL of the product",
            "example": "https://cdn.example.com/images/headphones.jpg"
          },
          "category": {
            "type": "string",
            "description": "Category of the product",
            "example": "Electronics"
          },
          "tags": {
            "description": "List of tags for filtering or search",
            "example": [
              "wireless",
              "audio",
              "bluetooth"
            ],
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "name",
          "price",
          "stock"
        ]
      },
      "UpdateProductDto": {
        "type": "object",
        "properties": {}
      }
    }
  }
