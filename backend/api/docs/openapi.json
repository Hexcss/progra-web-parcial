  "info": {
    "title": "API Template",
    "description": "Reusable NestJS API template for microservices",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "UpdateUserDto": {
        "type": "object",
        "properties": {
          "displayName": {
            "type": "string",
            "description": "Updated display name for the user",
            "example": "Jane Doe"
          },
          "avatarUrl": {
            "type": "string",
            "description": "URL of the new avatar image",
            "example": "https://cdn.example.com/avatars/janedoe.jpg",
            "format": "uri"
          }
        }
      },
      "AdminCreateUserDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "Email address of the user",
            "example": "user@example.com",
            "format": "email"
          },
          "password": {
            "type": "string",
            "description": "Password for the account (minimum 6 characters)",
            "example": "securePass123",
            "minLength": 6
          },
          "displayName": {
            "type": "string",
            "description": "Display name of the user",
            "example": "Jane Doe"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "AdminUpdateUserDto": {
        "type": "object",
        "properties": {
          "displayName": {
            "type": "string",
            "description": "Updated display name for the user",
            "example": "Jane Doe"
          },
          "avatarUrl": {
            "type": "string",
            "description": "URL of the new avatar image",
            "example": "https://cdn.example.com/avatars/janedoe.jpg",
            "format": "uri"
          }
        }
      },
      "RegisterDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "User email address",
            "example": "newuser@example.com",
            "format": "email"
          },
          "password": {
            "type": "string",
            "description": "Password (minimum 6 characters)",
            "example": "supersecret",
            "minLength": 6
          },
          "displayName": {
            "type": "string",
            "description": "Display name of the user",
            "example": "John Doe"
          }
        },
        "required": [
          "email",
          "password",
          "displayName"
        ]
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "User email address",
            "example": "user@example.com",
            "format": "email"
          },
          "password": {
            "type": "string",
            "description": "User password (minimum 6 characters)",
            "example": "strongpassword",
            "minLength": 6
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "CreateCategoryDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Laptops"
          },
          "icon": {
            "type": "string",
            "example": "laptop"
          }
        },
        "required": [
          "name",
          "icon"
        ]
      },
      "UpdateCategoryDto": {
        "type": "object",
        "properties": {}
      },
      "CreateProductDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "price": {
            "type": "number",
            "minimum": 0
          },
          "stock": {
            "type": "number",
            "minimum": 0
          },
          "imageUrl": {
            "type": "string"
          },
          "category": {
            "type": "string"
          },
          "categoryId": {
            "type": "string"
          },
          "tags": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "name",
          "price",
          "stock"
        ]
      },
      "UpdateProductDto": {
        "type": "object",
        "properties": {}
      },
      "CreateReviewDto": {
        "type": "object",
        "properties": {
          "productId": {
            "type": "string"
          },
          "score": {
            "type": "number",
            "minimum": 1,
            "maximum": 5,
            "example": 4.5
          },
          "comment": {
            "type": "string"
          }
        },
        "required": [
          "productId",
          "score"
        ]
      },
      "UpdateReviewDto": {
        "type": "object",
        "properties": {}
      },
      "CreateDiscountDto": {
        "type": "object",
        "properties": {
          "productId": {
            "type": "string"
          },
          "discountPercent": {
            "type": "number",
            "example": 15
          },
          "startDate": {
            "type": "string"
          },
          "endDate": {
            "type": "string"
          }
        },
        "required": [
          "productId",
          "discountPercent",
          "startDate",
          "endDate"
        ]
      },
      "UpdateDiscountDto": {
        "type": "object",
        "properties": {}
      },
      "CreateOrderItemDto": {
        "type": "object",
        "properties": {
          "productId": {
            "type": "string"
          },
          "quantity": {
            "type": "number"
          }
        },
        "required": [
          "productId",
          "quantity"
        ]
      },
      "CreateOrderDto": {
        "type": "object",
        "properties": {
          "items": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CreateOrderItemDto"
            }
          },
          "currency": {
            "type": "string"
          }
        },
        "required": [
          "items"
        ]
      },
      "UpdateOrderDto": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "enum": [
              "created",
              "processing",
              "shipped",
              "delivered",
              "cancelled",
              "canceled"
            ]
          }
        }
      }
    }
  }
